                    <img src="https://res.cloudinary.com/djxxgqird/image/upload/v1760967722/Screenshot_2025-10-20-18-38-24-76_1c337646f29875672b5a61192b9010f9_nixfmz.jpg" alt="Solution" id="solutionImage">
                </div>
            </div>

            <div class="game-completion" id="gameCompletion" style="display: none;">
                <div class="completion-message">
                    <h3>🎉 Congratulations! 🎉</h3>
                    <p>You solved the puzzle!</p>
                    <p>Time: <span id="finalTime"></span></p>
                    <p>Moves: <span id="finalMoves"></span></p>
                    <button class="game-btn" id="playAgainBtn">Play Again</button>
                </div>
            </div>
        </div>
    </section>

    <!-- Countdown Section -->
    <section id="countdown" class="countdown-section">
        <div class="container">
            <h2 class="section-title">⏰ Birthday Countdown</h2>
            <div class="countdown-container">
                <div class="countdown-input">
                    <label for="birthdayDate">Set Your Birthday Date:</label>
                    <input type="datetime-local" id="birthdayDate">
                </div>
                
                <div class="countdown-display" id="countdownDisplay">
                    <div class="time-unit">
                        <span class="time-number" id="days">78</span>
                        <span class="time-label">Days</span>
                    </div>
                    <div class="time-unit">
                        <span class="time-number" id="hours">18</span>
                        <span class="time-label">Hours</span>
                    </div>
                    <div class="time-unit">
                        <span class="time-number" id="minutes">11</span>
                        <span class="time-label">Minutes</span>
                    </div>
                    <div class="time-unit">
                        <span class="time-number" id="seconds">57</span>
                        <span class="time-label">Seconds</span>
                    </div>
                </div>

                <div class="countdown-message" id="countdownMessage"><p>🎂 78 days until your special day!</p></div>
            </div>
        </div>
    </section>

